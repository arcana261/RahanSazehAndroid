<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arcana.rahansazeh">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".RahanSazehApp"
        android:allowBackup="true"
        android:icon="@drawable/rahan_sazeh_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rahan_sazeh_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProjectSelectionActivity" />
        <activity android:name=".DataEntryActivity" />
        <activity android:name=".VerifyDataActivity"></activity>
    </application>

</manifest>